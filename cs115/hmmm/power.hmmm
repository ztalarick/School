#Computes x^y
0 read r1	#Read base from the user
1 read r2	#Read power from the user
2 setn r3 1	#set r3 = 1
3 jeqzn r2, 7	#if r2 == 0 jump to line 5
4 mul r3 r3 r1	#change r3 = r3 * r1 
5 addn r2 -1	#change r2 = r2 - 1
6 jumpn 3	#repeat
7 write r3	#write the answer
8 halt		#stop
